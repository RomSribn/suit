{
    "extends": [
        "react-app",
        "@typescript-eslint/parser",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
        "jsx-a11y/anchor-has-content": "off",
        "no-useless-escape": "off",
        "no-eval": "off"
    }
}
