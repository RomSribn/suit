.dummy-container
  display flex
  flex-flow column nowrap
  justify-content space-between
  position relative
  height 100%
  overflow hidden

  .circle
    height 27px
    width 27px
    border-radius 100%

  .controls
    display flex
    flex-flow row nowrap
    justify-content center
    align-items center
    position relative
    flex-basis 57px
    height 57px
    width 100%
    border-bottom 0

  .center
    margin 0 16px
